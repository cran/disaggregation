Package: disaggregation
Type: Package
Title: Disaggregation Modelling
Version: 0.2.0
Authors@R: c(
    person("Anita", "Nandi", email = "anita.k.nandi@gmail.com", role = "aut", comment = c(ORCID = "0000-0002-5087-2494")),
